{
  "name": "daming-select",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 8080",
    "build": "ng build select && ng build daming-select --prod --base-href ./",
    "build_lib:watch": "ng build select --watch",
    "build_lib": "ng build select --prod",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "copy-license": "node tools/copy-license.js",
    "copy-readme": "node tools/copy-readme.js",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/select && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
		"push": "ng build select && ng build daming-select --prod --base-href ./ && npx angular-cli-ghpages --dir=dist/daming-select",
		"postinstall": "ngcc",
    "preinstall": "node tools/check-yarn.js",
    "sass": "sass-lint -v -q",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "~9.0.0",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular-devkit/build-ng-packagr": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.5.3",
    "chalk": "^2.4.2",
    "codelyzer": "^5.1.2",
    "conventional-changelog-cli": "^2.0.23",
    "husky": "^4.2.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "sass-lint": "^1.13.1",
    "shelljs": "^0.8.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.7.5"
  }
}
