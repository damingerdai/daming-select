{
  "name": "daming-select",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 8080",
    "build": "ng build select && ng build daming-select --prod --base-href ./",
    "build_lib:watch": "ng build select --watch",
    "build_lib": "ng build select",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "copy-license": "node tools/copy-license.js",
    "copy-readme": "node tools/copy-readme.js",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/select && npm pack",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "push": "ng build select && ng build daming-select --prod --base-href ./ && npx angular-cli-ghpages --dir=dist/daming-select",
    "preinstall": "node tools/check-yarn.js",
    "sass": "sass-lint -v -q",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.1",
    "@angular/cdk": "~10.0.1",
    "@angular/common": "~10.0.1",
    "@angular/compiler": "~10.0.1",
    "@angular/core": "~10.0.1",
    "@angular/forms": "~10.0.1",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "~10.0.1",
    "@angular/platform-browser-dynamic": "~10.0.1",
    "@angular/router": "~10.0.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.1",
    "@angular/language-service": "~10.0.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.5.3",
    "chalk": "^2.4.2",
    "codelyzer": "^5.1.2",
    "conventional-changelog-cli": "^2.0.23",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "sass-lint": "^1.13.1",
    "shelljs": "^0.8.3",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "^3.9.5"
  }
}